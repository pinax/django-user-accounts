msgid "Password"
msgstr "Password"

#: forms.py:29
msgid "Password (again)"
msgstr "Password (di nuovo)"

#: forms.py:41
msgid "Usernames can only contain letters, numbers and underscores."
msgstr "Il nome utente può contenere solo lettere, numeri e trattini bassi."

